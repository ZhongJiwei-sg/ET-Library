    <Method Name="_readSensors" Id="{0ef14f12-ee5f-4508-a584-c135df9025d0}" FolderPath="methods\">
      <Declaration><![CDATA[METHOD PROTECTED _readSensors : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[	SensorBasePos(
		Force	:= Q_ToBasePos AND NOT InWorkPos AND(Options.0 OR SimulationOn),
		Invert	:= Options.5,
		PT_ON	:= SEL(Options.0 OR SimulationOn,PT_SensorDamping,PT_FEEDBACK),
		PT_OFF	:= SEL(Options.0 OR SimulationOn,PT_SensorDamping,T#0MS),
		Q_DIRECT=>,
		Q		=> InBasePos,
		Q_RT	=> ,
		Q_FT	=> ,
		IN		=> );


	SensorWorkPos(
		Force	:= Q_ToWorkPos AND NOT InBasePos AND(Options.1 OR SimulationOn),
		Invert	:= Options.6,
		PT_ON	:= SEL(Options.1 OR SimulationOn ,PT_SensorDamping,PT_FEEDBACK),
		PT_OFF	:= SEL(Options.1 OR SimulationOn ,PT_SensorDamping,T#0MS),
		Q_DIRECT=>,
		Q		=> InWorkPos,
		Q_RT	=> ,
		Q_FT	=> ,
		IN		=> );
		

	_readSensors									:= TRUE;]]></ST>
      </Implementation>
    </Method>
    <Method Name="_runtimeChk" Id="{f11c69b0-454d-49fe-a39d-4103bcdae74c}" FolderPath="methods\">
      <Declaration><![CDATA[METHOD PROTECTED _runtimeChk : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[(* Timeout WorkPos ************************************************)
	TonErrWorkPos(	IN := Q_ToWorkPos AND (NOT InWorkPos OR InBasePos),
					PT := MAX(PT_Timeout + PT_SensorDamping,PT_Feedback + T#1S),
					Q => ErrWorkPos);

(* Timeout WorkPos ************************************************)
	tonErrBasePos(	IN := Q_ToBasePos AND (NOT InBasePos OR InWorkPos),
					PT := MAX(PT_Timeout + PT_SensorDamping,PT_Feedback + T#1S),
					Q => ErrBasePos);
					
(*********************************************************************)
	_runtimeChk									:= TRUE;]]></ST>
      </Implementation>
    </Method>
    <Method Name="_writeStatus" Id="{49ba5128-4e45-4725-b063-43ad82d4e7f7}" FolderPath="methods\">
      <Declaration><![CDATA[METHOD PROTECTED _writeStatus : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[

	(* Move *)
	IF Q_ToBasePos AND NOT Q_ToWorkPos AND (NOT InBasePos OR InWorkPos)
	THEN
		Status.0 											:= NOT Options.4;
		Status.1											:= Options.4;
	ELSIF Q_ToWorkPos AND NOT Q_ToBasePos AND (NOT InWorkPos OR InBasePos)
	THEN
		Status.0											:= Options.4;
		Status.1											:= NOT Options.4;
	END_IF              									

	(* BasePos / WorkPos reached*)
	Status.2 												:= SEL(Options.4, InBasePos , InWorkPos);
	Status.3 												:= SEL(Options.4, InWorkPos , InBasePos);
 				        									
	(* Feed ok *)    									
	Status.4 												:= SEL(Options.4, FeedBasePos , FeedWorkPos);
	Status.5 												:= SEL(Options.4, FeedWorkPos , FeedBasePos);
                        									
	(* Ready *)         									
	Status.6												:= NOT Status.7; 
	                    									
	(* Error *)         									
	Status.7 												:= ErrBasePos OR ErrWorkPos OR ErrLogic;
	
	(* Logic error bit **********************************************)
	ErrLogic												:= InBasePos AND InWorkPos AND NOT Options.3;
	
	(* Result *)
	_writeStatus												:= TRUE;]]></ST>
      </Implementation>
    </Method>
    <Method Name="enable" Id="{e420b069-d630-4632-b1b4-2ef9990b28a6}" FolderPath="methods\">
      <Declaration><![CDATA[METHOD PUBLIC enable : BOOL //Return Done = TRUE if WorkPos is TRUE and InWorkPos = TRUE or WorkPos is FALSE and InBasePos is TRUE 
VAR_INPUT
	IN													:	BOOL; //TRUE=WORK / FALSE=BASE
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF IN
THEN
	Enable 													:= THIS^.moveToWorkPos();
ELSE
	Enable 													:= This^.moveToBasePos();
END_IF
]]></ST>
      </Implementation>
    </Method>
