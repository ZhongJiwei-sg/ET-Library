    <Method Name="moveToBasePos" Id="{0d109ec4-6cfd-4589-934b-8b13cdd735d3}" FolderPath="methods\">
      <Declaration><![CDATA[METHOD PUBLIC moveToBasePos : BOOL //Return = Done -> InBasePos
]]></Declaration>
      <Implementation>
        <ST><![CDATA[ToWorkPos 									:= Q_ToWorkPos	:= FALSE;
ToBasePos 													:= TRUE;
Q_ToBasePos	 												:= FeedBasePos AND (NOT Options.7 OR NOT InBasePos);
moveToBasePos 									:= Done			:= InBasePos;
Busy														:= NOT Done; ]]></ST>
      </Implementation>
    </Method>
    <Method Name="moveToWorkPos" Id="{dcebcec2-61a8-4bbc-aa95-e2e13080ef8f}" FolderPath="methods\">
