<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.13">
  <POU Name="Cylinder" Id="{c3fcaee2-da05-4234-9f2d-c86efbb08a63}" SpecialFunc="None">
    <Declaration><![CDATA[{attribute 'no_check'}
//Cylinder object with two outputs (Workpos./Basepos) and correlated inputs
//Via the Options byte, it is possible to disable the use either of one or both feedback inputs.
//In case the physical feedback input in disabled, the status output of the FB will be generated by PT_Feedback
//In case the physical feedback input will not be triggerd to his corresonding outout within a certain time (PT_Feedback), an timeout error occures
//The physical outputs could be blocked by set the FeedxxxPos signals to false.
//FB "INPUT" is used inside
FUNCTION_BLOCK Cylinder IMPLEMENTS I_Cylinder
VAR_OUTPUT CONSTANT
{attribute 'displaymode':='binary'}
	OPTION_DISABLE_SENSOR_BASEPOS							:	BYTE  := 2#0000_0001; //Disable sensor check basepos. Feedback time used instead to create signal
{attribute 'displaymode':='binary'}
	OPTION_DISABLE_SENSOR_WORKPOS							:	BYTE  := 2#0000_0010; //Disable sensor check workpos. Feedback time used instead to create signal
{attribute 'displaymode':='binary'}
	OPTION_FREE												:	BYTE  := 2#0000_0100; //Not used option		
{attribute 'displaymode':='binary'}
	OPTION_DISABLE_LOGIC_ERROR								:	BYTE  := 2#0000_1000; //Disable logic error (Sensor workpos and basepos active at the same time) 	
{attribute 'displaymode':='binary'}
	OPTION_SWAP_SENSOR_STATUS								:	BYTE  := 2#0001_0000; //Swap signals work and base pos at Status byte
{attribute 'displaymode':='binary'}
	OPTION_INVERT_SENSOR_BASEPOS							:	BYTE  := 2#0010_0000; //Invert logic of sensor basepos (->NO)
{attribute 'displaymode':='binary'}
	OPTION_INVERT_SENSOR_WORKPOS							:	BYTE  := 2#0100_0000;//Invert logic of sensor workpos (->NO)
{attribute 'displaymode':='binary'}
	OPTION_EN_IMPULS_VALVE									:	BYTE  := 2#1000_0000; //Outputs are switched off if endposition is reached	
END_VAR
VAR_INPUT  (* Peripherie Outputs *)
	Quit													:	BOOL;	//Resets error with rising edge
	ToWorkPos												:	BOOL;	//moves cylinder to work position
	ToBasePos												:	BOOL;	//moves cylinder to base position
{attribute 'displaymode':='binary'}         			
	Options													:	BYTE  := 2#0000_0000;  (* option constants are predefined, so they can accordingly be passed symbolically to the block
																							DISABLE_FEEDBACK_BP	= .0
																							DISABLE_FEEDBACK_WP	= .1
																							ENABLE_LATCH_ERROR	= .2
																							DISABLE_LOGIC_ERROR	= .3																							
																							SWAP_SENSOR_STATUS	= .4
																							FEEDBACK_BP_INVERT	= .5
																							FEEDBACK_WP_INVERT	= .6
																							IMPULS_VALVE		= .7
																									*)
	FeedBasePos												:	BOOL := TRUE;	//enables feed in base direction
	FeedWorkPos												:	BOOL := TRUE;	//enables feed in work direction
	PT_Timeout												:	TIME := T#5S; 	//Runtime check BasePos
	PT_Feedback												:	TIME := T#3S; 	//Delay simulate Feedback
	PT_SensorDamping										:	TIME := T#0MS; 	//Work and Base pos sensor on/off damping time to suppress sensor flickering  
END_VAR                                     			
VAR_OUTPUT  (* Peripherie Inputs *)         			
	InBasePos												:	BOOL;	//TRUE when cylinder is in base position
	InWorkPos												:	BOOL;	//TRUE when cylinder is in work position
	Done													:	BOOL; 	//Limit sensor reached or feedback time counted down
	Busy													:	BOOL;	//Busy is TRUE at the rising edge of Execute and FALSE when one of the outputs Done, Aborted, or Error is set 
	ErrBasePos												:	BOOL;	//Cannot reach base position
	ErrWorkPos												:	BOOL;	//Cannot reach work position  
	ErrLogic												:	BOOL; 	//Both feedback signals are on
{attribute 'displaymode':='binary'}         			
	Status													:	BYTE;	(* 	Bit 0 = Q_ToBasePos or opposite if status swap in Options is active
																			Bit 1 = Q_ToWorkPos or opposite if status swap in Options is active
																			Bit 2 = InBasePos or opposite if status swap in Options is active
																			Bit 3 = InWorkPos or opposite if status swap in Options is active
																			Bit 4 = FeedBasePos or opposite if status swap in Options is active
																			Bit 5 = FeedWorkPos or opposite if status swap in Options is active
																			Bit 6 = Ready (no error)
																			Bit 7 = Error timeout move to position or sensor logic
																		*)
	Q_ToBasePos										AT  %Q* :	BOOL; //Hardware output - only with read access
	Q_ToWorkPos										AT  %Q* :	BOOL; //Hardware output - only with read access
END_VAR
VAR// IOs 
	SensorBasePos											: INPUT;
	SensorWorkPos											: INPUT;	
	TonErrBasePos											: TON;
	TonErrWorkPos											: TON;
	SimulationOn											:	BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[	(* Cylinder	 IO V2.9					 								       	*)
	(* Includes functionblock "INPUT" 												*)
	(********************************************************************************)

(* Write Output *****************************************************)
	IF ToBasePos 	
	THEN
		moveToBasePos();
	ELSIF ToWorkPos AND NOT ToBasePos
	THEN
		moveToWorkPos();
	ELSE
		switchOff();
	END_IF

(* Reedkontakte lesen ***********************************************)
	this^._readSensors();
	
(* Status *************************************************)
	_writeStatus();
                        	
(* ack Error **********************************************************)
	IF Quit  THEN  reset(); END_IF

(* Timeout  ************************************************)
	_runtimeChk();
	
(*********************************************************************)]]></ST>
    </Implementation>
