ToWorkPos													:= TRUE;
Q_ToWorkPos	 												:= FeedWorkPos AND (NOT Options.7 OR NOT InWorkPos);
moveToWorkPos								:= Done 				:= InWorkPos;
Busy														:= NOT Done;]]></ST>
      </Implementation>
    </Method>
