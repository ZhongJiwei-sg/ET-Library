	Window				:	VALUE_3D;
END_VAR


]]></Declaration>
      <Implementation>
        <ST><![CDATA[InPosXY 													:= CAM(CheckPos.X, X ,Window.X)
																AND CAM(CheckPos.Y, Y ,Window.Y);]]></ST>
      </Implementation>
    </Method>
    <Method Name="InPosXZ" Id="{fc0c51c6-84b1-4e1f-b81a-d008205f6885}">
