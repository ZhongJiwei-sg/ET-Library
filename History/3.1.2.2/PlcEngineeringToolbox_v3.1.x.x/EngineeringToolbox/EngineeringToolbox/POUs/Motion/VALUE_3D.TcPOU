    <Method Name="copy" Id="{ddd0e79e-0c68-4b87-bbbb-03add5d3ec00}">
      <Declaration><![CDATA[METHOD PUBLIC copy : BOOL //Return value = TRUE if execution finished
VAR_IN_OUT
	Source													:	VALUE_3D;
END_VAR


]]></Declaration>
      <Implementation>
        <ST><![CDATA[	Valid													:= Source.Valid;
	X														:= Source.X;
	Y														:= Source.Y;
	Z														:= Source.Z;
	copy													:= TRUE;
	]]></ST>
      </Implementation>
    </Method>
    <Method Name="delete" Id="{e27f2f82-fa0e-415c-881f-2fdf55727c99}">
      <Declaration><![CDATA[METHOD PUBLIC delete : BOOL //Return value = TRUE if execution finished - Valid bit will not change
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[	X					 									:= 0.0;	
	Y					 									:= 0.0;	
	Z					 									:= 0.0;	
	delete													:= TRUE;]]></ST>
      </Implementation>
    </Method>
    <Method Name="InPos" Id="{3f819b1f-98c5-4273-9d49-dfed9f1e422a}">
      <Declaration><![CDATA[METHOD PUBLIC InPos : BOOL //Return value = TRUE axes is within window of checkPos values
VAR_IN_OUT
	CheckPos												:	VALUE_3D;
	Window													:	VALUE_3D;
END_VAR

]]></Declaration>
      <Implementation>
        <ST><![CDATA[InPos														:= CAM(CheckPos.X, X ,Window.X)
																AND CAM(CheckPos.Y, Y ,Window.Y)
																AND CAM(CheckPos.Z, Z ,Window.Z);
																	]]></ST>
      </Implementation>
    </Method>
    <Method Name="InPosByAxisNo" Id="{10338d23-16a1-48a7-a3d8-ef9ded08fff4}">
      <Declaration><![CDATA[METHOD PUBLIC InPosByAxisNo : BOOL //Return value = TRUE selected axis is within window of checkPos values
VAR_INPUT
	No					:	INT;
END_VAR
VAR_IN_OUT
	CheckPos			:	VALUE_3D;
	Window				:	VALUE_3D;
END_VAR

]]></Declaration>
      <Implementation>
        <ST><![CDATA[InPosByAxisNo													:= (CAM(CheckPos.X, X ,Window.X) OR No <> 1)
																AND (CAM(CheckPos.Y, Y ,Window.Y) OR No <> 2)
																AND (CAM(CheckPos.Z, Z ,Window.Z) OR No <> 3);]]></ST>
      </Implementation>
    </Method>
    <Method Name="InPosXY" Id="{12a7a23b-9d0f-4dc7-9c9b-00152ab9608e}">
