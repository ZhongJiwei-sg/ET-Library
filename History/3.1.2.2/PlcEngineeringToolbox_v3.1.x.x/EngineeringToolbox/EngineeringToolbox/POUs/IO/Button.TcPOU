    <Method Name="TGL" Id="{65570dd6-f3cd-474e-8e29-c687ca40f8d0}">
      <Declaration><![CDATA[METHOD PUBLIC TGL : booL //Return value = Q_TGL status
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[TGL := Q_TGL 														:= NOT Q_TGL;]]></ST>
