<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.13">
  <POU Name="Button" Id="{8d336af3-4a9d-4b3c-8acf-c17d18307046}" SpecialFunc="None">
    <Declaration><![CDATA[{attribute 'no_check'}
FUNCTION_BLOCK Button 
VAR_INPUT
	Enable													:	BOOL := TRUE;	//FB will be processed as long as enable is TRUE 
	Force													:	BOOL := FALSE; 	//simulates Input signal
	Invert													:	BOOL := FALSE; 	//inverts the Hardware Input
	LED												AT  %Q*	:	BOOL;			//LED/Light hardware output
END_VAR         									
VAR_OUTPUT
	Q														:	BOOL;	//direct signal
	Q_RT													:	BOOL;	//R_Trig signal of delay Q
	Q_FT													:	BOOL;	//F_Trig signal of delay Q
	Q_TGL													:	BOOL;	//Toggle -> use flag Q_RT
	In												AT  %I*	:	BOOL; 	//Hardware Input
END_VAR
VAR
	Mem														:	BOOL;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[(* Taster mit Togglefunktion V1.0						*)
(********************************************************)

(********************************************************)
Mem															:= Q;
Q 															:= ((In OR Force) XOR Invert) AND Enable;
Q_RT 														:= NOT Mem AND Q;
Q_FT														:= Mem AND NOT Q;
Q_TGL 														:= (Q_RT XOR Q_TGL);
(********************************************************)]]></ST>
    </Implementation>
    <Method Name="setDisable" Id="{5217cc95-77d1-45cb-b2be-7fa2aff8d790}">
      <Declaration><![CDATA[METHOD PUBLIC setDisable : BOOL
